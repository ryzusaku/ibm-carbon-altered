  "tokenColors": [
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#be95ff" // Purple 40
      }
    },
    {
      "scope": "constant.language.boolean.true",
      "settings": {
        "foreground": "#42be65" // Green 40
      }
    },
    {
      "scope": "constant.language.boolean.false",
      "settings": {
        "foreground": "#ff8389" // Red 40
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#646695"
      }
    },
    {
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#87eded" // Teal 20
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#be95ff" // Purple 40
      }
    },
    {
		"scope": "entity.other.attribute-name",
		"settings": {
			"foreground": "#87eded" // Teal 20
		}
    },
    //tutaj
    {
		"scope": [
			"meta.class.inheritance",
			"meta.function-call",
			"keyword.control.import",
			"keyword.control.from",
			"entity.name.tag",
		],
		"settings": {
			"foreground": "#be95ff" // Purple 40
		}
	},
	{
		"scope": [
			"constant.numeric",
			"variable.other.enummember",
			"keyword.operator.plus.exponent",
			"keyword.operator.minus.exponent",
			"entity.name.function",
			"meta.member.access",
			"entity.other.attribute-name",
			"constant.other",
		],
		"settings": {
			"foreground": "#78a9ff" // Blue 40
		}
	},
{
	"scope": [
		"entity.other.attribute-name.class.css",
		"entity.other.attribute-name.class.mixin.css",
		"entity.other.attribute-name.id.css",
		"entity.other.attribute-name.parent-selector.css",
		"entity.other.attribute-name.pseudo-class.css",
		"entity.other.attribute-name.pseudo-element.css",
		"source.css.less entity.other.attribute-name.id",
		"entity.other.attribute-name.scss",
		"meta.embedded.assembly",
		"string",
		"storage.type",
		"entity.name",
		"keyword.control",
	],
	"settings": {
		"foreground": "#ff7eb6" // Magenta 40
	}
},
{
	"scope": [
		"meta.function-call.arguments",
	],
	"settings": {
		"foreground": "#ffffff", // White
	}
},
{
	"scope": [
		"variable.parameter.function.language.special",
		"variable.language.special",
		"support.class.component",
	],
	"settings": {
		"foreground": "#ffffff", // White
		"fontStyle": "bold"
	}
},
{
	"scope": [
		"punctuation.separator.key-value.html",
		"support.function.misc.css",
		"storage.type.class",
		"punctuation.accessor.js",
		"punctuation.separator.period",
		"punctuation.separator.parameters",
		"keyword.operator",
		"storage.type.function.arrow",
	],
	"settings": {
		"foreground": "#87eded" // Teal 20
	}
},
{
	"scope": [
		"support.function.magic",
	],
	"settings": {
		"foreground": "#78a9ff", // Blue 40
		"fontStyle": "bold"
	}
},
{
	"scope": "invalid",
	"settings": {
		"foreground": "#F44747"
	}
},
{
	"scope": "markup.underline",
	"settings": {
		"foreground": "#87eded", // Teal 20
		"fontStyle": "underline"
	}
},
{
		"scope": "markup.bold",
		"settings": {
			"foreground": "#87eded", // Teal 20
			"fontStyle": "bold"
		}
